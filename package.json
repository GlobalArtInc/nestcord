{
	"name": "@globalart/nestcord",
	"description": "A module for creating Discord bots using NestJS, based on Discord.js",
	"version": "2.3.1",
	"private": false,
	"scripts": {
		"build": "rimraf dist && tsc -p tsconfig.build.json",
		"prepublish:npm": "npm run build",
		"publish:npm": "release-it",
		"prepublish:dev": "npm run build",
		"publish:dev": "npm publish --access public --tag dev",
		"prepare": "husky ./.github/husky",
		"format": "prettier --write \"packages/**/*.ts\"",
		"lint": "eslint --ignore-path .gitignore packages/**/*.ts",
		"upgrade": "npx npm-check-updates -u"
	},
	"lint-staged": {
		"*.ts": "npm run format"
	},
	"directories": {
		"packages": "packages",
		"examples": "examples",
		"docs": "docs"
	},
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"keywords": [
		"nest",
		"nestjs",
		"nestcord",
		"nestjs-module",
		"discord",
		"discordjs",
		"discord bot",
		"discord bot api",
		"bot",
		"bot api",
		"bot framework"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/GlobalartInc/nestcord"
	},
	"dependencies": {
		"@globalart/text-utils": "^1.0.4",
		"@nestjs/axios": "^4.0.0",
		"@nestjs/schedule": "^6.0.0",
		"axios": "^1.9.0",
		"cron": "4.3.3",
		"deepmerge": "^4.3.1",
		"lavalink-client": "^2.5.3",
		"path-to-regexp": "^8.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "19.8.1",
		"@commitlint/config-angular": "19.8.1",
		"@favware/npm-deprecate": "2.0.0",
		"@nestjs/common": "11.1.5",
		"@nestjs/core": "11.1.6",
		"@nestjs/platform-express": "^11.1.1",
		"@types/node": "24.2.0",
		"@typescript-eslint/eslint-plugin": "8.39.0",
		"@typescript-eslint/parser": "8.39.0",
		"discord-api-types": "0.38.18",
		"discord.js": "14.21.0",
		"eslint": "9.32.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-prettier": "5.5.4",
		"husky": "9.1.7",
		"lint-staged": "16.1.4",
		"prettier": "3.6.2",
		"reflect-metadata": "0.2.2",
		"release-it": "19.0.4",
		"rimraf": "6.0.1",
		"rxjs": "7.8.2",
		"ts-node": "10.9.2",
		"typescript": "5.8.3"
	},
	"peerDependencies": {
		"@nestjs/common": ">=10.2.0",
		"@nestjs/core": ">=10.2.0",
		"discord.js": ">=14.0.1",
		"reflect-metadata": ">=0.2.1",
		"rxjs": ">=7.2.0"
	},
	"engines": {
		"node": ">=16.6.0",
		"npm": ">=7.0.0"
	}
}
